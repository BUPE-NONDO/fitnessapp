{
  "name": "@fitness-app/shared",
  "version": "0.1.0",
  "private": true,
  "description": "Shared utilities, types, and schemas for FitnessApp",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./schemas": "./src/schemas/index.ts",
    "./types": "./src/types/index.ts",
    "./utils": "./src/utils/index.ts"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo node_modules"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@fitness-app/eslint-config": "workspace:*",
    "@types/node": "^20.11.24",
    "eslint": "^8.57.0",
    "typescript": "^5.4.2"
  }
}
